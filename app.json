{
  "expo": {
    "name": "УлГТУ",
    "slug": "улгту",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#007AFF"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#007AFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font"
    ]
  }
}
